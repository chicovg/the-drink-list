{
  "name": "the-beer-list",
  "version": "2.0.0",
  "description": "A [reagent](https://reagent-project.github.io/) application designed to track and rate beers!",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@pathofdev/react-tag-input": "^1.0.7",
    "@yaireo/tagify": "^4.9.2",
    "bulma": "^0.9.3",
    "bulma-slider": "^2.0.4",
    "material-icons": "^1.10.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-tag-autocomplete": "^6.3.0",
    "shadow-cljs": "^2.16.6",
    "spectre.css": "^0.5.9"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@storybook/addon-actions": "^6.4.7",
    "@storybook/addon-essentials": "^6.4.7",
    "@storybook/addon-links": "^6.4.7",
    "@storybook/builder-webpack5": "^6.4.7",
    "@storybook/manager-webpack5": "^6.4.7",
    "@storybook/react": "^6.4.7",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "clean-css-cli": "^5.4.2",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-cljs-test": "^0.1.0",
    "node-sass": "^6.0.1",
    "postcss-cli": "^9.0.2"
  },
  "scripts": {
    "css:compile": "node-sass --output-style=expanded --source-map=true sass/site.sass public/css/site.css",
    "css:prefix": "postcss public/css/site.css --use=autoprefixer --map=false --output=public/css/site.css",
    "css:minify": "cleancss --level=1 --source-map --source-map-inline-sources --output public/css/site.min.css public/css/site.css",
    "css:watch": "npm run css:compile -- --watch",
    "css:build": "npm run css:compile && css:prefix && css:minify",
    "dev": "shadow-cljs watch app",
    "dev-stories": "shadow-cljs watch stories",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chicovg/the-beer-list.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chicovg/the-beer-list/issues"
  },
  "homepage": "https://github.com/chicovg/the-beer-list#readme"
}
